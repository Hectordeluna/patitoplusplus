programa expresiones;
var
  int i, j, p;
  int Arreglo[10];
  int Matriz[10][8];
  int T;

funcion int fact (int j)
var int i; {
  i = j + (p - j * 2 + j);
  si (j == 0.3) entonces {
    regresa(j);
  } sino {
    regresa (j * fact(j - 1));
  }
}

funcion void inicia (int y)
var int x;
int t; {
  x = -1;
  mientras (x < 11) haz {
    Arreglo[x] = y * x;
  }
}

principal() {
  Arreglo[0] = 1;
  escribe(Arreglo + Arreglo);
  desde (Arreglo[0] = 0) hasta (Arreglo[0] < 10) hacer {
    escribe(Arreglo[0]);
  }
}