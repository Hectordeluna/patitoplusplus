funcion int fibo(int x)
{
  si (x == 1 || x == 0) entonces{
    regresa(x);
  }
  sino{
    regresa(fibo(x-1) + fibo(x-2));
  }
}


principal()
{
  i = 0;
  mientras (i < 10) haz {
    j = 0;
    mientras (j < 10) haz {
      A[i][j] = 1;
      B[i][j] = 4;
      j = j + 1;
    }
    i = i + 1;
  }
  B = A + B * B;
  i = 0;
}